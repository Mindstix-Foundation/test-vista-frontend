    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "cropperjs": "^1.6.2",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
